<mvc:View controllerName="sap.pieces.controller.vh.ValueHelp"
          xmlns:mvc="sap.ui.core.mvc"
          xmlns="sap.m"
          xmlns:fb="sap.ui.comp.filterbar"
          height="100%">
    <Page
            showNavButton="true"
            navButtonPress="onNavBack">
        <content>
            <fb:FilterBar id="filterbar" useToolbar="false" showGoOnFB="false">
                <fb:filterItems>
                    <fb:FilterItem name="process" label="Process">
                        <fb:control>
                            <MultiInput
                                    id="multiInput"
                                    valueHelpOnly="true"
                                    valueHelpRequest=".onValueHelpRequested"/>
                        </fb:control>
                    </fb:FilterItem>
                </fb:filterItems>
            </fb:FilterBar>
        </content>
    </Page>
</mvc:View>
