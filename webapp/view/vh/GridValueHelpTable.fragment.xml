<core:FragmentDefinition
        xmlns="sap.ui.table"
        xmlns:core="sap.ui.core"
        xmlns:m="sap.m"
        height="100%">
    <Table rows="{/}">
        <columns>
            <Column>
                <m:Label text="Product Id"/>
                <template>
                    <m:Text text="{ProductId}"/>
                </template>
            </Column>
            <Column>
                <m:Label text="Name"/>
                <template>
                    <m:Text text="{Name}"/>
                </template>
            </Column>
        </columns>
    </Table>
</core:FragmentDefinition>
