<mvc:View
        xmlns="sap.m"
        xmlns:mvc="sap.ui.core.mvc">
    <Table
            id="table11"
            width="auto"
            items="{ooo>/}">
        <columns>
            <Column id="isbnColumn">
                <Text text="ISBN"/>
            </Column>
            <Column id="nameColumn">
                <Text text="Name"/>
            </Column>
        </columns>
        <items>
            <ColumnListItem
                    type="Navigation"
                    press="onPress">
                <cells>
                    <Text text="{ooo>Name}"/>
                    <Text text="{ooo>SupplierName}"/>
                </cells>
            </ColumnListItem>
        </items>
    </Table>
</mvc:View>