<mvc:View
        controllerName="sap.pieces.controller.Pieces"
        xmlns="sap.m"
        xmlns:mvc="sap.ui.core.mvc">
    <Page title="SAP UI5 Pieces">
        <content>
            <GenericTile class="sapUiTinyMargin tileLayout" header="Processes" press="onNavToProcess"/>

            <GenericTile class="sapUiTinyMargin tileLayout" header="Library" press="onNavToLibrary"/>

            <GenericTile class="sapUiTinyMargin tileLayout" header="Graphic" subheader="SAP UI5 Graphic" press="onNavToGraphic"/>

            <GenericTile class="sapUiTinyMargin tileLayout" header="Ajax" press="onNavToAjax"/>

            <GenericTile class="sapUiTinyMargin tileLayout" header="JSONModel" press="onNavToJsonModel"/>
        </content>
    </Page>
</mvc:View>
